{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "outDir": "dist",
    "declaration": true,
    "emitDeclarationOnly": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "sourceMap": false,
    "types": ["@cloudflare/workers-types"]
  },
  "include": ["src"]
}
